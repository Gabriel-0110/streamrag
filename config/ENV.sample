# Copy to .env and fill with your values

# OpenAI
OPENAI_API_KEY=sk-...
OPENAI_MODEL=gpt-4o-mini

# Supabase
SUPABASE_URL=https://<project>.supabase.co
# Use service role for ingestion (writes)
SUPABASE_SERVICE_ROLE_KEY=...
# Use anon for app (reads)
SUPABASE_ANON_KEY=...

# Embeddings
EMBEDDING_MODEL=text-embedding-3-small

# App tuning
CHUNK_SIZE=1000
CHUNK_OVERLAP=200
MAX_RESULTS=5
